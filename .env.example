# Kinesis On-Demand Demo - Local Development Environment Variables
# Copy this file to .env and update with your values

# AWS Configuration
# AWS credentials (AWS_ACCESS_KEY_ID, AWS_SECRET_ACCESS_KEY, AWS_SESSION_TOKEN) 
# are automatically passed from your system environment variables
# No need to set them here - just ensure they're available in your shell
AWS_REGION=us-east-1

# Kinesis Configuration
STREAM_NAME=social-media-stream

# Demo Traffic Configuration

PER_TASK_CAPACITY=1000
MAX_TASKS=100

# Container Metrics Configuration
CLOUDWATCH_NAMESPACE=KinesisOnDemandDemo
SERVICE_NAME=kinesis-data-generator
CLUSTER_NAME=kinesis-demo-local
ENVIRONMENT=development
DEPLOYMENT_ID=docker-compose
CONTAINER_ID=local-generator-1
METRICS_PUBLISH_INTERVAL=10
ENABLE_CLOUDWATCH_METRICS=true

# Controller Mode (for local development use 'internal')
CONTROLLER_MODE=internal
PARAMETER_PREFIX=/kinesis-demo

# Logging Configuration
LOG_LEVEL=INFO

# Python Configuration
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1